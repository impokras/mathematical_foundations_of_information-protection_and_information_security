   "source": [
    "function fermat_test(n)\n",
    "    a = rand(2:n-2)\n",
    "    if powermod(a,(n-1), n) != 1\n",
    "        return \"$n is composite\"\n",
    "    end\n",
    "    return \"$n is probably prime\" \n",
    "end\n",
    "\n",
    "\n",
    "n = 11\n",
    "\n",
    "fermat_test(n)"
   ]
  }
